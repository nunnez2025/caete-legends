{
  "name": "caete-legends",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Jogo de cartas épico inspirado no folclore brasileiro",
  "keywords": [
    "jogo",
    "cartas",
    "folclore",
    "brasileiro",
    "react",
    "threejs"
  ],
  "author": "Caeté Legends Team",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "docker:build": "docker build -t caete-legends .",
    "docker:run": "docker run -p 3000:3000 caete-legends",
    "docker:dev": "docker-compose up app-dev",
    "docker:prod": "docker-compose up app-prod",
    "prepare": "husky install"
  },
  "dependencies": {
    "@questlabs/react-sdk": "latest",
    "@react-three/drei": "^10.7.3",
    "@react-three/fiber": "^9.3.0",
    "@react-three/postprocessing": "^2.15.11",
    "@supabase/supabase-js": "^2.39.0",
    "@types/three": "^0.165.0",
    "@use-gesture/react": "^10.3.0",
    "axios": "^1.6.7",
    "date-fns": "4.1.0",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.2",
    "framer-motion": "^11.0.8",
    "gsap": "^3.12.5",
    "lucide-react": "^0.540.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.1.0",
    "react-spring": "^10.0.1",
    "react-use-measure": "^2.1.1",
    "terser": "^5.43.1",
    "three": "^0.165.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.9",
    "@babel/preset-typescript": "^7.23.9",
    "@eslint/js": "^9.9.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^29.7.0",
    "cssnano": "^6.0.5",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.49",
    "postcss-preset-env": "^9.4.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.1.2",
    "vite": "^5.4.2",
    "vite-bundle-analyzer": "^0.7.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
